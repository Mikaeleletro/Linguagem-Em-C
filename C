#include <stdio.h> 
int main() { 
   float a, b, c, d; 
   printf("Qual o valor da Largura?\n");
   scanf("%f", &a);
   printf("Qual o Comprimento?\n");
   scanf("%f", &b);
   printf("Qual o valor do M quadrado?\n");
   scanf("%f", &c);
   d = (a * b) / c;
   printf("o Valor a ser pago é %.2f", d);
   return 0;
}

#include <stdio.h> 
